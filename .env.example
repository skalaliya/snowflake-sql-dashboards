# Snowflake Connection Configuration
# Copy this file to .env and fill in your actual values

# Account identifier (e.g., xy12345.us-east-1)
SNOW_ACCOUNT=your_account_identifier

# User credentials
SNOW_USER=your_username
SNOW_PASSWORD=your_password

# Optional: Role to use (if not specified, will use default role)
SNOW_ROLE=ACCOUNTADMIN

# Database and schema to deploy to
SNOW_DATABASE=TPCH_DASHBOARDS
SNOW_SCHEMA=PUBLIC

# Optional: Warehouse to use for deployment
SNOW_WAREHOUSE=COMPUTE_WH
